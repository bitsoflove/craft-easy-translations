{
    "name": "bitsoflove/translation",
    "description": "Plugin to manage translations. Export and import functionality.",
    "type": "craft-plugin",
    "version": "0.0.1",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "translation"
    ],
    "support": {
        "docs": "https://github.com/bitsoflove/translation/blob/master/README.md",
        "issues": "https://github.com/bitsoflove/translation/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "bitsoflove",
            "homepage": "https://www.bitsoflove.be/"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0"
    },
    "autoload": {
        "psr-4": {
          "bitsoflove\\translation\\": "src/"
        }
    },
    "extra": {
        "name": "Translation",
        "handle": "translation",
        "developer": "bitsoflove",
        "developerUrl": "https://www.bitsoflove.be/",
        "documentationUrl": "https://github.com/bitsoflove/translation/blob/master/README.md",
        "changelogUrl": "https://raw.githubusercontent.com/bitsoflove/translation/master/CHANGELOG.md",
        "class": "bitsoflove\\translation\\Translation"
    }
}
